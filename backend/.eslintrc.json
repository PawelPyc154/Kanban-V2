{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-useless-escape": "off",
    "no-underscore-dangle": "off",
    "func-names": "off",
    "implicit-arrow-linebreak": "off",
    "object-curly-newline": "off"
  }
}
